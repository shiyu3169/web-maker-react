        "axios": "^0.18.0",
        "bcrypt-nodejs": "0.0.3",
        "concurrently": "^4.0.1",
        "cookie-parser": "^1.4.3",
        "express": "^4.16.3",
        "express-session": "^1.15.6",
        "mongoose": "^5.3.1",
        "multer": "^1.4.0",
